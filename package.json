{
  "name": "booking-system",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/api",
    "apps/web",
    "scripts"
  ],
  "scripts": {
    "dev:api": "npm run dev --workspace=apps/api",
